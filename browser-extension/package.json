{
  "name": "redblink-browser",
  "displayName": "RedBlink - Browser Extension",
  "description": "Non-intrusive AI debugging assistant for browsers",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RedBlink/RedBlink.git"
  },
  "homepage": "https://redblink.dev",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.css\" \"src/**/*.html\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist out"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.27",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.8.0",
    "@google/generative-ai": "^0.3.0",
    "axios": "^1.6.0",
    "openai": "^4.24.0"
  }
}
